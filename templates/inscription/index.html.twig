{% extends 'base.html.twig' %}
{% block title %}Inscription{% endblock %}

{% block body %}
    <h1>Inscription</h1>

    <table class="table">
        <thead>
            <tr>
                <th>ID</th>
                <th>nom</th>
                <th>Post Nom</th>
                <th>Prenom</th>
                 <th>Sexe</th>
                 <th>Age</th>
                 <th>Status</th>
                 <th>Roles</th>
                <th>Adresse</th>
            </tr>
        </thead>
        <tbody>
        {% for user in users %}
            <tr>
                <td>{{ user.id }}</td>
                <td>{{ user.nom }}</td>
                <td>{{ user.postnom }}</td>
                <td>{{ user.prenom }}</td>
                <td>{{ user.sexe }}</td>
                <td>{{ user.age }}</td>
                <td>{{ user.status }}</td>
                <td>{{ user.roles }}</td>
                <td>{{ user.adresse }}</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

{% endblock %}